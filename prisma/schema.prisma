// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShortenedUrl {
  id          String   @id @default(cuid())
  code        String   @unique
  originalUrl String   @db.Text

  @@index([code])
}